{
  "version": "0.2",
  "language": "en-US",
  "ignorePaths": [
    ".github/workflows/**",
    ".vite-ssg-temp",
    "**/*.json",
    "**/*.jsonc",
    "**/CHANGELOG.md",
    "**/coverage/**",
    "**/dist/**",
    "**/node_modules/**",
    "CHANGELOG.md",
    "coverage",
    "dist/",
    "exported-game-data",
    "images/**/index.ts",
    "LICENSE",
    "pnpm-lock.yaml",
    "pnpm-lock.yaml",
    "scripts/parse-raw-game-data/data/",
    "scripts/parse-raw-game-data/docs/raw-collection-parser.ts",
    "src/auto-imports.d.ts",
    "src/components.d.ts"
  ],
  "dictionaries": [
    "en_US",
    "filetypes",
    "misc",
    "node",
    "npm",
    "softwareTerms",
    "typescript"
  ],
  "import": ["@cspell/dict-cryptocurrencies/cspell-ext.json"],
  "ignoreRegExpList": [
    "/[^\\s@]+@([^\\s@]+\\.)+[^\\s@]{2,4}/",
    "/[A-Fa-f0-9]{6}/",
    "/[A-Za-z0-9]{32,}/",
    "// @ts-.*",
    "/\\STS\\S+/",
    "/`[^`]*`/",
    "/``[\\w\\W]*?``/",
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "/TS[^\\s]+/",
    "\\(#.+?\\)",
    "<svg.*>.*</svg>"
  ],
  "words": [
    "Alclad",
    "bar",
    "baz",
    "Biofuel",
    "buildable",
    "buildables",
    "callbag",
    "caterium",
    "composables",
    "consts",
    "corge",
    "corge",
    "demi",
    "emojione",
    "esbuild",
    "ficsit",
    "ficsmas",
    "foo",
    "fred",
    "garply",
    "grault",
    "iconify",
    "imagetools",
    "intlify",
    "Jetpack",
    "ko-fi",
    "kofi",
    "litecoin",
    "monero",
    "Nobelisk",
    "noreply",
    "Paleberry",
    "pinia",
    "plugh",
    "pnpm",
    "Quickwire",
    "quux",
    "qux",
    "Rebar",
    "Rebecca",
    "rebeccastevens",
    "scannable",
    "scannables",
    "sonarjs",
    "tetromino",
    "thud",
    "Turbofuel",
    "unassert",
    "unocss",
    "unplugin",
    "unproject",
    "unprojected",
    "unprojecting",
    "Vite",
    "vitejs",
    "Vitesse",
    "vitest",
    "vueuse",
    "waldo",
    "Xeno",
    "xyzzy",
    "Zipline"
  ],
  "overrides": [
    {
      "filename": "**/*.{vue,ts,js}",
      "ignoreRegExpList": ["/@[a-z]+/", "/#(end)?region/"],
      "includeRegExpList": [
        "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
        "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
      ]
    },
    {
      "filename": "locales/*.yml",
      "ignoreRegExpList": ["/([a-z0-9]|-|_|\"|')+:/ugi"],
      "dictionaries": [],
      "words": []
    },
    {
      "filename": "locales/en.yml",
      "language": "en"
    },
    {
      "filename": "locales/en-US.yml",
      "language": "en-US"
    },
    {
      "filename": "locales/en-GB.yml",
      "language": "en-GB"
    }
  ]
}
