  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-ssg": "vite-ssg build",
    "dev": "vite",
    "get-game-images": "node --loader=ts-paths-esm-loader/transpile-only --experimental-specifier-resolution=node  ./scripts/get-game-images/index.ts",
    "lint": "pnpm run lint:js && pnpm run lint:md && pnpm run lint:prettier && pnpm run lint:spelling",
    "lint:js": "eslint .",
    "lint:js-fix": "eslint . --fix",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "lint:prettier": "prettier \"**/*\" --ignore-unknown --list-different",
    "lint:prettier-fix": "prettier \"**/*\" --ignore-unknown --write",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "parse-raw-game-data": "node --loader=ts-paths-esm-loader/transpile-only --experimental-specifier-resolution=node  ./scripts/parse-raw-game-data/index.ts",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "type-check": "pnpm run type-check:app && pnpm run type-check:node && pnpm run type-check:scripts",
    "type-check:app": "vue-tsc -p src/tsconfig.json --noEmit --skipLibCheck",
    "type-check:node": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "type-check:scripts": "tsc -p scripts/tsconfig.json --noEmit --skipLibCheck"
  },
