{
  "name": "satisfactory-power-tools",
  "private": true,
  "homepage": "https://github.com/RebeccaStevens/satisfactory-power-tools#readme",
  "bugs": {
    "url": "https://github.com/RebeccaStevens/satisfactory-power-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RebeccaStevens/satisfactory-power-tools"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/rebeccastevens"
    }
  ],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebecca.stevens@outlook.co.nz"
  },
  "type": "module",
  "scripts": {
    "build": "rimraf build && pnpm run generate && NODE_ENV=production react-router build",
    "cz": "git-cz",
    "dev": "react-router dev",
    "generate": "pnpm run generate:peggy && pnpm run generate:game-data",
    "generate:game-data": "tsx ./app/game-data/generate/index.ts",
    "generate:peggy": "peggy -o app/game-data/generate/peggy/collection.js --allowed-start-rules List,Map --dts --format es app/game-data/generate/peggy/collection.peggy",
    "lint": "eslint && pnpm run lint:md && pnpm run lint:spelling && pnpm run lint:knip && pnpm run lint:packages",
    "lint-fix": "eslint --fix && pnpm run lint:md-fix && pnpm run lint:packages-fix",
    "lint:js": "eslint \"**/*.?([cm])[jt]s?(x)\"",
    "lint:js-fix": "eslint \"**/*.?([cm])[jt]s?(x)\" --fix",
    "lint:knip": "knip",
    "lint:md": "markdownlint-cli2",
    "lint:md-fix": "markdownlint-cli2 --fix",
    "lint:md-full": "pnpm run lint:md && eslint \"**/*.md\"",
    "lint:md-full-fix": "pnpm run lint:md-fix && eslint \"**/*.md\" --fix",
    "lint:packages": "pnpm dedupe --check",
    "lint:packages-fix": "pnpm dedupe",
    "lint:spelling": "cspell lint --no-progress --show-suggestions --show-context --dot \"**\" \".github/**/*\"",
    "lint:yaml": "eslint \"**/*.y?(a)ml\"",
    "lint:yaml-fix": "eslint \"**/*.y?(a)ml\" --fix",
    "prepare": "husky && pnpm run generate",
    "start": "NODE_ENV=production react-router-serve ./build/server/index.js",
    "test": "pnpm run test:js",
    "test:js": "vitest --coverage",
    "test:js-run": "vitest run --coverage",
    "typecheck": "pnpm run typegen && tsc",
    "typegen": "react-router typegen"
  },
  "dependencies": {
    "@react-router/node": "7.1.1",
    "@react-router/serve": "7.1.1",
    "dedent": "1.5.3",
    "devalue": "5.1.1",
    "effect": "3.12.0",
    "effect-uom": "1.0.1",
    "isbot": "5.1.18",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.1",
    "type-fest": "4.30.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.26.0",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@cspell/dict-cryptocurrencies": "5.0.3",
    "@eslint-react/eslint-plugin": "1.22.1",
    "@eslint/compat": "1.2.4",
    "@react-router/dev": "7.1.1",
    "@rebeccastevens/eslint-config": "3.5.1",
    "@stylistic/eslint-plugin": "2.12.1",
    "@tailwindcss/vite": "4.0.0-beta.8",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "@vitest/coverage-v8": "3.0.0-beta.3",
    "@vitest/eslint-plugin": "1.1.20",
    "babel-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "browserslist": "4.24.3",
    "chai": "5.1.2",
    "commitizen": "4.3.1",
    "cspell": "8.17.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-flat-config-utils": "0.4.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-merge-processors": "0.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-format": "0.1.3",
    "eslint-plugin-functional": "7.2.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-jsonc": "2.18.2",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-yml": "1.16.0",
    "husky": "9.1.7",
    "jsonc-eslint-parser": "2.4.0",
    "knip": "5.41.1",
    "lint-staged": "15.2.11",
    "markdownlint-cli2": "0.16.0",
    "peggy": "4.2.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "sharp": "0.33.5",
    "tailwindcss": "4.0.0-beta.8",
    "tsc-files": "1.1.4",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vite": "6.0.5",
    "vite-plugin-babel": "1.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.0-beta.3",
    "yaml-eslint-parser": "1.2.3"
  },
  "packageManager": "pnpm@9.15.1",
  "pnpm": {
    "ignoredOptionalDependencies": [
      "@img/sharp-libvips-*"
    ]
  }
}
