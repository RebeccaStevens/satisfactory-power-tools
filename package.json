{
  "name": "satisfactory-power-tools",
  "private": true,
  "license": "BSD-3-Clause",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint && pnpm run lint:css && pnpm run lint:md && pnpm run lint:spelling && pnpm run lint:knip && pnpm run lint:packages",
    "lint-fix": "eslint --fix && pnpm run lint:css-fix && pnpm run lint:md-fix && pnpm run lint:packages-fix",
    "lint:css": "stylelint \"**/*.?(s)css\"",
    "lint:css-fix": "stylelint --fix \"**/*.?(s)css\"",
    "lint:css-full": "eslint \"**/*.?(s)css\" && pnpm run lint:css",
    "lint:css-full-fix": "eslint \"**/*.?(s)css\" --fix && pnpm run lint:css-fix",
    "lint:js": "eslint \"**/*.?([cm])[jt]s?(x)\"",
    "lint:js-fix": "eslint \"**/*.?([cm])[jt]s?(x)\" --fix",
    "lint:knip": "knip",
    "lint:md": "markdownlint-cli2",
    "lint:md-fix": "markdownlint-cli2 --fix",
    "lint:md-full": "pnpm run lint:md && eslint \"**/*.md\"",
    "lint:md-full-fix": "pnpm run lint:md-fix && eslint \"**/*.md\" --fix",
    "lint:packages": "pnpm dedupe --check",
    "lint:packages-fix": "pnpm dedupe",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "lint:yaml": "eslint \"**/*.y?(a)ml\"",
    "lint:yaml-fix": "eslint \"**/*.y?(a)ml\" --fix",
    "prepare": "husky",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest",
    "test-run": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remix-run/node": "2.13.1",
    "@remix-run/react": "2.13.1",
    "@remix-run/serve": "2.13.1",
    "chai": "5.1.2",
    "isbot": "5.1.17",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.15.2",
    "@eslint/compat": "1.2.2",
    "@rebeccastevens/eslint-config": "3.3.3",
    "@remix-run/dev": "2.13.1",
    "@rollup/plugin-strip": "3.0.4",
    "@stylistic/eslint-plugin": "2.10.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.12.2",
    "@typescript-eslint/parser": "8.12.2",
    "@vitest/coverage-v8": "2.1.6",
    "@vitest/eslint-plugin": "1.1.7",
    "browserslist": "4.24.2",
    "cspell": "8.15.5",
    "cssnano": "7.0.6",
    "cssnano-preset-advanced": "7.0.6",
    "dedent": "1.5.3",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-flat-config-utils": "0.4.0",
    "eslint-formatting-reporter": "0.0.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-merge-processors": "0.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-format": "0.1.2",
    "eslint-plugin-functional": "7.1.0",
    "eslint-plugin-import-x": "4.4.0",
    "eslint-plugin-jsdoc": "50.4.3",
    "eslint-plugin-jsonc": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.12.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "eslint-plugin-readable-tailwind": "1.8.1",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-yml": "1.15.0",
    "husky": "9.1.6",
    "jsonc-eslint-parser": "2.4.0",
    "knip": "5.36.1",
    "lint-staged": "15.2.10",
    "markdownlint-cli2": "0.14.0",
    "postcss": "8.4.47",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "10.0.9",
    "postcss-url": "10.1.3",
    "rollup-plugin-deassert": "1.3.0",
    "sort-package-json": "2.10.1",
    "stylelint": "16.10.0",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-high-performance-animation": "1.10.0",
    "stylelint-no-unsupported-browser-features": "8.0.2",
    "tailwindcss": "3.4.14",
    "tsc-files": "1.1.4",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.6",
    "yaml-eslint-parser": "1.2.3"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=22.9.0"
  }
}
